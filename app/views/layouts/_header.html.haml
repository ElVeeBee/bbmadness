    %ul
      %li
        %a.navbar-brand{:href => "#"}
          Brackets #{Time.now.strftime("%Y")}
      %li
        = link_to 'Home', root_path
      %li
        = link_to 'My Brackets', my_brackets_path
      %li
        = link_to 'Edit Profile', edit_user_registration_path(current_user)
